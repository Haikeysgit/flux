{
  "name": "flux",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-s db:setup start:dev",
    "dev:fast": "next dev",
    "start:dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint",
    "db:setup": "prisma db push && run-s db:seed",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma db push --force-reset && run-s db:seed",
    "db:studio": "prisma studio",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@solana/web3.js": "^1.98.4",
    "@tanstack/react-query": "^5.90.19",
    "bs58": "^6.0.0",
    "lucide-react": "^0.562.0",
    "next": "16.1.3",
    "prisma": "^5.22.0",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.3",
    "npm-run-all2": "^8.0.4",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
